{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Dragdroppaste",
  "id": "grafana-dragdroppaste-app",
  "backend": true,
  "executable": "gpx_dragdroppaste",
  "info": {
    "keywords": ["app"],
    "description": "",
    "author": {
      "name": "Grafana"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Datasets",
      "path": "/a/%PLUGIN_ID%/datasets",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Dataset",
      "path": "/a/%PLUGIN_ID%/dataset",
      "addToNav": false,
      "defaultNav": false
    },
    {
      "type": "page",
      "icon": "cog",
      "name": "Configuration",
      "path": "/plugins/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  },
  "extensions": { "addedHooks": [
    {"title": "foo", "targets": ["dashboard/grid"]}
  ]
  }
}
